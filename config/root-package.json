{
  "name": "weev-final",
  "version": "1.0.0",
  "description": "Weev final project",
  "main": "Frontend/main.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "cd Frontend && python3 -m http.server 3001",
    "backend": "cd Backend && source ../backend_venv/bin/activate && uvicorn main:app --reload --port 8003",
    "frontend": "cd Frontend && npm run dev",
    "dev-all": "npx concurrently \"npm run backend\" \"npm run frontend\"",
    "install-deps": "cd Frontend && npm install"
  },
  "devDependencies": {
    "typescript": "^5.5.2",
    "concurrently": "^8.2.2"
  },
  "type": "module"
}
